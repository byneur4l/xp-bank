{
  "name": "xp-case-back",
  "version": "1.0.0",
  "description": "XP Inc. Tech Case - Daily representation of XP backend's",
  "main": "index.ts",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma",
    "seed": "ts-node --transpile-only src/prisma/seeders/index.ts"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register ./src/index.ts",
    "ext": "ts, js"
  },
  "scripts": {
    "dev": "nodemon ./src/index.ts",
    "eslint": "eslint . --ext .ts",
    "dbfs": "bash get_sql_fileName.sh && bash set_dbName.sh && bash drop_db.sh && prisma db push && prisma db seed",
    "dbcreate": "prisma db push",
    "dbreset": "bash drop_db.sh && prisma db push",
    "printrospec": "prisma db pull && prisma generate",
    "dbschema": "prisma db push",
    "dbmigrate": "prisma migrate dev",
    "dbseed": "prisma db seed",
    "pump": "bash drop_db.sh && prisma db push && prisma db seed",
    "test": "jest",
    "build": "tsc --build",
    "clean": "tsc --clean"
  },
  "keywords": [],
  "author": "Lucas Barbosa dos Reis - neur4l - 15/07/2022",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.0.5",
    "@types/nodemon": "^1.19.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.2.7",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^28.1.3",
    "prisma": "^4.0.0",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@prisma/client": "^4.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-rescue": "^1.1.31",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.19",
    "swagger-ui-express": "^4.5.0",
    "zod": "^3.17.9"
  }
}
